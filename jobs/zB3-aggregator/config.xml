  <builders>
    <hudson.tasks.Shell>
      <command>cp -f org.eclipse.ecf/releng/org.eclipse.ecf.releng.bm/ecf.rmap ${WORKSPACE}
cp -f org.eclipse.ecf/releng/org.eclipse.ecf.releng.bm/rmap2b3aggr.xsl ${WORKSPACE}

xsltproc --stringparam repository ${REPOSITORY} rmap2b3aggr.xsl ecf.rmap &gt; ecf.b3aggr
cat ecf.b3aggr

${HUDSON_HOME}/tools/Buckminster_3.6/b3aggr/b3 aggregate --buildModel ecf.b3aggr --action CLEAN_BUILD --buildRoot ${WORKSPACE}

# remove garbage
rm -rf ${WORKSPACE}/final/p2.packed/
rm ${WORKSPACE}/final/artifacts.jar
rm ${WORKSPACE}/final/content.jar

# copy the rest
mkdir -p ${OUTPUT}
cp -rf ${WORKSPACE}/final/* ${OUTPUT}
</command>
    </hudson.tasks.Shell>
