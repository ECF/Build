    <hudson.tasks.Shell>
      <command>${HUDSON_HOME}/tools/Buckminster_3.6/director/director -r http://download.eclipse.org/releases/indigo/,http://download.eclipse.org/eclipse/updates/3.7/,http://self.pluginbuilder.org/nightly/p2-updateSite,http://download.eclipse.org/technology/swtbot/helios/dev-build/update-site -d ${WORKSPACE}/tests/ -i org.eclipse.sdk.ide,org.pluginbuilder.autotestsuite.feature.feature.group,org.eclipse.swtbot.eclipse.feature.group -profile SDKProfile

